import java.io.*;
import java.util.*;
class rec
{
public static void main(String args[])
{
int rows;
Scanner s = new Scanner(System.in);
System.out.println("enter no.of rows:");
rows = s.nextInt();
for(int i=1;i<=rows;i++)
{
for(int j=1;j<=i;j++)
{
System.out.print(i + " ");
}
System.out.println();
}
}
}
